{
  "name": "react-flex-layout",
  "version": "1.0.0",
  "description": "Layout Components for React based on Flexbox",
  "main": "lib/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
