  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "assign-styles": "^1.0.2",
    "inline-style-prefixer": "^0.5.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  }
